    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/eslint": "^8.56.1",
    "@types/jest": "^29.5.11",
    "@types/prettier": "^3.0.0",
    "eslint": "^8.56.0",
    "is-wsl": "^3.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "puppeteer": "^21.7.0",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
